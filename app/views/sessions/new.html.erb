
	<div class="container" >
		<form action="/online_lending" method="post" class="form-signin" id="login">
			<h2 class="form-signin-heading">Login</h2>
			<input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
			<input type="email" name="email" placeholder="Email" id="inputEmail" class="form-control" required autofocus>
			<input type="password" name="password" placeholder="Password" id="inputPassword" class="form-control" required>
			<div class="checkbox">
				<label>
					<input type="checkbox" value="remember-me"> Remember me
				</label>
			</div>
			<button class="btn btn-lg btn-primary btn-block" type="submit">Login</button>
			<!-- <input type="submit" value="Login"> -->
			<p class="form-signin"><a href="/online_lending/register">Click here to register</a></p>
		</form><br>
	</div>




