<div class="col-md-6 col-sm-8 center_div_horizontal login" >
	<form action="/online_lending" method="post" class="form-signin" id="login">
		<h2 class="text-center">Login</h2>
		<input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
		<input type="email" name="email" placeholder="Email" id="inputEmail" class="form-control my_margin_bottom" required autofocus>
		<div class="form-group">
			<input type="password" name="password" placeholder="Password" id="inputPassword" class="form-control" required>
		</div>
		<div class="form-group">
			<button class="btn btn-lg btn-primary btn-block" type="submit">Login</button>
		</div>
		<p class="form-signin"><a href="/online_lending/register" class="my_white">Click here to register</a></p>
	</form>
</div> 




